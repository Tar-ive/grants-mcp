{
  "mcpServers": {
    "grantsmanship-local": {
      "command": "python3",
      "args": ["/Users/tarive/Desktop/grants-data-analysis/grants-mcp/main.py"],
      "env": {
        "SIMPLER_GRANTS_API_KEY": "YOUR_API_KEY_HERE",
        "MCP_TRANSPORT": "stdio"
      }
    },
    "grantsmanship-docker": {
      "command": "npx",
      "args": [
        "mcp-remote",
        "http://localhost:8081/mcp"
      ],
      "env": {
        "SIMPLER_GRANTS_API_KEY": "YOUR_API_KEY_HERE"
      }
    }
  }
}